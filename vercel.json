{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
          "distDir": "staticfiles_build"
      }
  }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app/wsgi.py"
    }
  ]
}
